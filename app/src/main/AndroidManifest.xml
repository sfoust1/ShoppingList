<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.projects.ShoppingList">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_cart2"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_cart2_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.projects.ShoppingList.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.projects.ShoppingList.DashboardActivity">
        </activity>
        <activity android:name="com.projects.ShoppingList.ItemActivity">
        </activity>
    </application>

</manifest>